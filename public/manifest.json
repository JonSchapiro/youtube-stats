{
  "short_name": "Schapchat",
  "name": "Schapchat Youtube Tool",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Schapchat Youtube Tool"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "chrome://favicon/",
    "storage"
  ],
  "background": {
    "page": "index.html"
 },
  "version": "1.0"
}